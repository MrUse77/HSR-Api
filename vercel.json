{
  "version": 2,
  "builds": [
    {
      "src": "FrontEnd/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "FrontEnd/dist"
      }
    },
    {
      "src": "Backend/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Backend/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "FrontEnd/dist/index.html"
    }
  ]
}
